    "javascript",
    "chrome",
    "firefox",
    "opera",
    "ie",
    "internet-explorer",
    "android",
    "rtcweb",
    "rtcmulticonnection.js",
    "multirtc",
    "webrtc-experiment",
    "javascript-library",
    "muaz",
    "muaz-khan"
  ],
  "dependencies": {
    "animejs": "^2.0.1",
    "async": "^2.1.5",
    "clipboard": "^1.6.1",
    "cross-env": "^3.1.4",
    "dragula": "^3.7.2",
    "express": "^4.14.1",
    "fs-extra": "^2.0.0",
    "jquery": "^3.1.1",
    "load-json-file": "^2.0.0",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "plyr": "^2.0.11",
