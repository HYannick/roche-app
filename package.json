    "socketio-over-nodejs": "^3.0.1",
    "write-json-file": "^2.0.0"
  },
  "analyze": false,
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template-pug": "^1.1.1",
    "npm-run-all": "^4.0.2",
